{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "browser_style": true,
      "default_icon": "icon_32.png",
      "default_popup": "popup.html"
   },
   "commands": {
      "toggle-interruption": {
         "description": "Toggle interruption",
         "suggested_key": {
            "default": "Ctrl+Shift+U",
            "mac": "Command+Shift+U"
         }
      }
   },
   "description": "IDM Browser Integration extension replaces default download manager by Internet Download Manager on Linux .",
   "icons": {
      "128": "icon_128.png",
      "32": "icon_32.png",
      "48": "icon_48.png",
      "64": "icon_64.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "19.0",
   "name": "IDM Integration",
   "offline_enabled": true,
   "permissions": [ "\u003Call_urls>", "webRequest", "webRequestBlocking", "nativeMessaging", "contextMenus", "downloads", "storage", "cookies", "activeTab" ],
   "short_name": "KGet",
   "version": "0.1"
}
